# DoubleZero Rewarder Environment Variables
# Copy this file to .env and fill in the values

# ============================================
# Calculator Settings
# ============================================

# Log level for calculator (debug, info, warn, error). Optional.
CALCULATOR__LOG_LEVEL=debug

# Shapley calculation parameters. Required.
CALCULATOR__SHAPLEY__OPERATOR_UPTIME=0.98
CALCULATOR__SHAPLEY__CONTIGUITY_BONUS=5.0
CALCULATOR__SHAPLEY__DEMAND_MULTIPLIER=1.2

# ============================================
# Ingestor Settings
# ============================================

# Log level for ingestor (debug, info, warn, error). Optional.
INGESTOR__LOG_LEVEL=info

# RPC endpoints. Required.
INGESTOR__RPC__URL="your_rpc_url"
INGESTOR__RPC__SOLANA_URL="https://api.mainnet-beta.solana.com"

# Program IDs (Devnet example - replace). Required.
INGESTOR__PROGRAMS__SERVICEABILITY_PROGRAM_ID="GYhQDKuESrasNZGyhMJhGYFtbzNijYhcrN9poSqCQVah"
